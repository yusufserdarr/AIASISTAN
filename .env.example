# API Anahtarlari ve Gizli Bilgiler
# Kendi degerlerinizi girin ve bu dosyayi .env olarak yeniden adlandirin

# OpenRouter API Anahtari (https://openrouter.ai/ adresinden alabilirsiniz)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Twilio Hesap Bilgileri (https://www.twilio.com/ adresinden alabilirsiniz)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Flask Gizli Anahtari (guvenlik icin rastgele bir string girin)
SECRET_KEY=your_secret_key_here

# Flask Ortam Ayarlari
FLASK_ENV=development
FLASK_DEBUG=TrueMY_PHONE_NUMBER=your_phone_number_here
